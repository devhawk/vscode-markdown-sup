{
	"name": "markdown-sup",
	"version": "0.0.0",
	"displayName": "Markdown Superscript",
	"description": "Adds ^superscript^ syntax support to VS Code's built-in markdown preview",
	"keywords": [
		"markdown",
		"superscript"
	],
	"icon": "logo.png",
	"publisher": "DevHawk",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/devhawk/vscode-markdown-sup.git"
	},
	"bugs": {
		"url": "https://github.com/devhawk/vscode-markdown-sup/issues"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"markdown.markdownItPlugins": true
	},
	"scripts": {
		"setversion": "gulp setversion",
		"package": "npm run setversion && vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"gulp": "^4.0.2",
		"nerdbank-gitversioning": "^3.1.91",
		"vsce": "^1.75.0"
	},
	"dependencies": {
		"markdown-it-sup": "^1.0.0"
	}
}
